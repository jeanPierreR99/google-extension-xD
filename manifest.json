{
   "manifest_version": 3,
   "name": "BABAS.DEV",
   "version": "1.0",
   "description": "alerta al cargar la pagina.",
   "icons": {
    "128": "babas.png"
   },
   "action":{
    "default_popup": "index.html"
   },
   "content_scripts": [
      {
         "matches": ["http://*/*", "https://*/*"],
         "js": ["content.js"]
      }
   ]
}